{
  "version": "0.1.0",
  "command": "npm",
  "isShellCommand": true,
  "showOutput": "silent",
  "suppressTaskName": true,
  "echoCommand": true,
  "tasks": [
    {
      "taskName": "build",
      "args": ["test"],
      "isBuildCommand": true,
      "problemMatcher": [
        "$tsc"
      ]
    },
    {
      "taskName": "load",
      "args": ["run", "load"]
    }
  ]
}