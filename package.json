{
  "name": "csgo-ranking-graph",
  "version": "1.0.0",
  "description": "Ranking graph for CSGO teams.",
  "main": "loader.js",
  "dependencies": {},
  "devDependencies": {
    "better-console": "^0.2.4",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "tslint": "^4.2.0",
    "typescript": "^2.0.7",
    "typings": "^1.5.0"
  },
  "scripts": {
    "tslint_graph": "tslint -c tslint.json --project graph/tsconfig.json -t stylish",
    "tsc_graph": "tsc -p graph/tsconfig.json",
    "build": "npm run tslint_graph && npm run tsc_graph",
    "tslint_loader": "tslint -c tslint.json --project loader/tsconfig.json -t stylish",
    "tsc_loader": "tsc -p loader/tsconfig.json",
    "load": "npm run tslint_loader && npm run tsc_loader && node loader/loader",
    "test": "npm run load && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencoveney/CsgoRankingsGraph.git"
  },
